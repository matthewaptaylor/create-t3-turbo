# Since .env is gitignored, you can use .env.example to build a new `.env` file when you clone the repo.
# Keep this file up-to-date when you add new variables to \`.env\`.

# This file will be committed to version control, so make sure not to have any secrets in it.
# If you are cloning this repo, create a copy of this file named `.env` and populate it with your secrets.

## Vite variables begin with VITE_
VITE_APP_NAME="Acme"

## Expo variables begin with EXPO_PUBLIC_
EXPO_PUBLIC_TITLE="Acme"

## Server variables have no prefix
DATABASE_USER="acme"
DATABASE_PASSWORD= # Generated with 'openssl rand -base64 32' on Unix
DATABASE_HOST="localhost"
DATABASE_PORT="3306"
DATABASE_NAME="acme"
DATABASE_URL="mysql://${DATABASE_USER}:${DATABASE_PASSWORD}@${DATABASE_HOST}:${DATABASE_PORT}/${DATABASE_NAME}"

MAILJET_API_KEY=
MAILJET_API_SECRET=

SUPERTOKENS_CONNECTION_URI="http://localhost:3567"
SUPERTOKENS_API_KEY= # Generated with 'openssl rand -base64 32' on Unix
SUPERTOKENS_APP_NAME="Acme"

API_URI="http://localhost:3002"
WEBSITE_URI="http://localhost:5173"
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
