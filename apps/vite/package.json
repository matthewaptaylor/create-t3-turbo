{
  "name": "@acme/vite",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc && pnpm run with-env vite build",
    "dev": "pnpm run with-env vite",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "ui:add": "pnpm dlx shadcn-ui add",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@acme/api": "workspace:^0.1.0",
    "@acme/auth": "workspace:^0.1.0",
    "@acme/db": "workspace:^0.1.0",
    "@acme/validators": "workspace:^0.1.0",
    "@fontsource-variable/noto-sans": "^5.0.1",
    "@radix-ui/react-slot": "^1.0.2",
    "@t3-oss/env-core": "^0.9.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.338.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:^0.2.0",
    "@acme/prettier-config": "workspace:^0.1.0",
    "@acme/tailwind-config": "workspace:^0.1.0",
    "@acme/tsconfig": "workspace:^0.1.0",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/vite",
      "@acme/eslint-config/react"
    ]
  },
  "prettier": "@acme/prettier-config"
}
